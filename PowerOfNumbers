class Solution {
    public int reverseExponentiation(int n) {
        if(rev(n) == 1) {
			return n;
		}
		else {
			double Doubleans  = Math.pow(n, n);
			int ans = (int) Doubleans;
			return ans;
		}
        
    }
    
    private static int rev(int n) {
		int rev =0;
		while(n>0) {
			int rem = n%10;
			rev = (rev *10) + rem;
			n =n/10;
		}
		
		
		return rev;
	}
}
